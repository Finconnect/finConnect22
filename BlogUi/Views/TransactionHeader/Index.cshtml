@model List<BlogUi.Models.CreditTransferInstruction>;
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_master.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Transactions To grouping in header
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr>
                            <td>pmtIdEndToEndId</td>
                            <td>pmtIdTxId</td>
                            <td>intrBkSttlmAm</td>
                            <td>chrgBr</td>
                            <td>instInf</td>
                            <td>cdTxId</td>
                            <td>msgDir</td>
                            <td>ccyId</td>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var m in Model)
                        {
                            <tr class="odd gradeX">
                                <td>@m.pmtIdEndToEndId</td>
                                <td>@m.pmtIdTxId</td>
                                <td>@m.intrBkSttlmAm</td>
                                <td>@m.chrgBr</td>
                                <td>@m.instInf</td>
                                <td>@m.cdTxId</td>
                                <td>@m.msgDir</td>
                                <td>@m.ccyId</td>


                            </tr>

                        }



                    </tbody>
                </table>


            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>






@section Scripts {
<script>
    $(document).ready(function () {
        $('#dataTables-example').DataTable({
            responsive: true
        });
    });
</script>
    }